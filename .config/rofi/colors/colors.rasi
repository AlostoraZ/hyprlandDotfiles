* {
    /* Background shades */
    bg0: #1a1110;
    bg1: #231918;
    bg2: #271d1c;
    bg3: #322826;

    /* Foreground shades */
    fg0: #f1dfdc;
    //fg0: #dec48c;
    fg1: #561e16;
    fg2: #d8c2be;
    fg3: #534341;

    /* Material theme mapping (optional for Waybar modules) */
    primary: #ffb4a8;
    secondary: #e7bdb6;
    tertiary: #dec48c;
    error: #ffb4ab;
    outline: #a08c89;
}
