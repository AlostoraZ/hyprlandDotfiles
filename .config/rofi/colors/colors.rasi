* {
    /* Background shades */
    bg0: #1a1110;
    bg1: #231918;
    bg2: #271d1c;
    bg3: #322826;

    /* Foreground shades */
    fg0: #f1dedc;
    //fg0: #e0c38c;
    fg1: #561e1a;
    fg2: #d8c2bf;
    fg3: #534341;

    /* Material theme mapping (optional for Waybar modules) */
    primary: #ffb4ac;
    secondary: #e7bdb8;
    tertiary: #e0c38c;
    error: #ffb4ab;
    outline: #a08c8a;
}
